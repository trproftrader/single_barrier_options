LogCoefficients = t.List[float]

# C++'taki -INFINITY karÅŸÄ±lÄ±ÄŸÄ±
NEGATIVE_INF = -math.inf

# ------------------------------------------------------------------
# ðŸŒŸ Log Binomial Precomputation (Stable)
# ------------------------------------------------------------------
# Calculates ln(C(n, j)) for j = 0 to n using log factorials
def precompute_log_C(n: int) -> LogCoefficients:
    """ln(C(n, j)) deÄŸerlerini Ã¶nceden hesaplar."""
    if n < 0:
        return []

    # 1. Pre-calculate ln(i!) for i = 0 to n
    log_factorial = [0.0] * (n + 1)
    # log_factorial[0] = 0.0 # ln(0!) = ln(1) = 0
    for i in range(1, n + 1):
        log_factorial[i] = log_factorial[i - 1] + math.log(float(i))

    # 2. Calculate ln(C(n, j)) = ln(n!) - ln(j!) - ln((n-j)!)
    log_C = [0.0] * (n + 1)
    log_factorial_n = log_factorial[n]
    for j in range(n + 1):
        log_C[j] = log_factorial_n - log_factorial[j] - log_factorial[n - j]
    return log_C

# ------------------------------------------------------------------
# âš¡ Optimized & Stable Vanilla Put/Call (YardÄ±mcÄ± Fonksiyon)
# Not: C++'taki OpenMP yerine Python'da temel list comprehension ve map/multiprocessing kullanÄ±labilir.
# Ancak birebir Ã§eviri iÃ§in dÃ¶ngÃ¼ yapÄ±sÄ± korunmuÅŸtur.
# ------------------------------------------------------------------

def _calculate_common_params(n: int, r: float, v: float, T: float) -> t.Tuple[float, float, float, float, float, float]:
    """Ortak parametreleri hesaplar: dt, u, d, p, q, disc, ln_u, ln_d."""
    dt = T / n
    sqrt_dt = math.sqrt(dt)
    u = math.exp(v * sqrt_dt)
    d = math.exp(-v * sqrt_dt)
    
    er_dt = math.exp(r * dt)
    p = (er_dt - d) / (u - d)
    q = 1.0 - p
    disc = math.exp(-r * T)
    
    ln_u = math.log(u)
    ln_d = math.log(d)
    
    return dt, u, d, p, q, disc, ln_u, ln_d

def myputprice(n: int, S: float, K: float, r: float, v: float, T: float, log_C: LogCoefficients) -> float:
    """Optimize edilmiÅŸ ve Stabil Vanilya Put opsiyonu fiyatlamasÄ±."""
    dt, u, d, p, q, disc, ln_u, ln_d = _calculate_common_params(n, r, v, T)

    ln_p = math.log(p)
    ln_q = math.log(q)
    ln_S = math.log(S)
    
    # KÄ±rÄ±lma noktasÄ± indeksi beta
    beta = (math.log(K / S) - n * ln_d) / (ln_u - ln_d)
    
    sum_val = 0.0
    end_j = math.ceil(beta)
    
    # C++'taki index sÄ±nÄ±rlamalarÄ±
    end_j = max(0, min(n, int(end_j)))

    # C++'taki OpenMP #pragma omp parallel for reduction(+:sum) karÅŸÄ±lÄ±ÄŸÄ±.
    # Python'da GIL nedeniyle saf dÃ¶ngÃ¼ bu kadar hÄ±zlÄ± olmayabilir. Performans iÃ§in
    # NumPy veya elle threading/multiprocessing gerekir. Birebir Ã§eviri iÃ§in basit dÃ¶ngÃ¼ kullanÄ±lÄ±r.
    for j in range(end_j + 1):
        log_coeff = log_C[j]
        
        # Calculate log(Probability)
        log_prob = log_coeff + j * ln_p + (n - j) * ln_q
        
        # Calculate Stock Price (ST)
        log_ST = ln_S + j * ln_u + (n - j) * ln_d
        ST = math.exp(log_ST)
        
        if ST < K:
            prob = math.exp(log_prob)
            sum_val += prob * (K - ST)
            
    return disc * sum_val

def mycallprice(n: int, S: float, K: float, r: float, v: float, T: float, log_C: LogCoefficients) -> float:
    """Optimize edilmiÅŸ ve Stabil Vanilya Call opsiyonu fiyatlamasÄ±."""
    dt, u, d, p, q, disc, ln_u, ln_d = _calculate_common_params(n, r, v, T)

    ln_p = math.log(p)
    ln_q = math.log(q)
    ln_S = math.log(S)
    
    # KÄ±rÄ±lma noktasÄ± indeksi alfa
    alfa = (n / 2.0) - (math.log(S / K) / (2.0 * v * math.sqrt(T / n)))
    
    sum_val = 0.0
    start_j = math.ceil(alfa)
    
    # C++'taki index sÄ±nÄ±rlamalarÄ±
    start_j = max(0, min(n + 1, int(start_j))) # n+1, range(start_j, n+1) iÃ§in
    
    if start_j > n:
        return 0.0

    # OpenMP yerine basit dÃ¶ngÃ¼
    for j in range(start_j, n + 1):
        log_coeff = log_C[j]
        
        # Calculate log(Probability)
        log_prob = log_coeff + j * ln_p + (n - j) * ln_q
        
        # Calculate Stock Price (ST)
        log_ST = ln_S + j * ln_u + (n - j) * ln_d
        ST = math.exp(log_ST)
        
        if ST > K:
            prob = math.exp(log_prob)
            sum_val += prob * (ST - K)
            
    return disc * sum_val

