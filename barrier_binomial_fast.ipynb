            if ST > K:
                final_prob = math.exp(log_final_prob)
                sum_val += final_prob * (ST - K)
                
    return disc * sum_val

def upoutcallprice(n: int, Barrier: float, S: float, K: float, r: float, v: float, T: float, log_C: LogCoefficients) -> float:
    """GeliÅŸmiÅŸ Stabilite: Up-and-Out Call opsiyonu fiyatlamasÄ±."""
    dt, u, d, p, q, disc, ln_u, ln_d = _calculate_common_params(n, r, v, T)

    ln_p = math.log(p)
    ln_q = math.log(q)
    ln_S = math.log(S)

    alfa = (n / 2.0) - (math.log(S / K) / (2.0 * v * math.sqrt(T / n)))

    # Reflection index H (Up-Out iÃ§in H > 0)
    H = 0
    Su = S * u
    for i in range(1, n + 1):
        if Su > Barrier:
            H = i
            break
        Su *= u # Su = S * u^(i+1)

    sum_val = 0.0
    start_j = math.ceil(alfa)
    start_j = max(0, min(n + 1, int(start_j)))
    
    if start_j > n:
        return 0.0

    # OpenMP yerine basit dÃ¶ngÃ¼
    for j in range(start_j, n + 1):
        # Main coefficient is C(n, j)
        log_coeff_main = log_C[j]
        
        # Reflected coefficient is C(n, n-j + H)
        idx_sub = n - j + H
        log_coeff_sub = log_C[idx_sub] if (0 <= idx_sub <= n) else NEGATIVE_INF

        # Apply Reflection Principle in LOG space for stability
        log_coeff_ratio = log_coeff_sub - log_coeff_main # ln(B/A)
        
        if log_coeff_ratio < 0.0:
            
            # Stable calculation of ln(A - B)
            log_effective_coeff = log_coeff_main + math.log(1.0 - math.exp(log_coeff_ratio))

            # Calculate log(Probability) (excluding C)
            log_prob_no_C = j * ln_p + (n - j) * ln_q
            
            # Combined log-probability
            log_final_prob = log_effective_coeff + log_prob_no_C
            
            # Calculate Stock Price (ST)
            log_ST = ln_S + j * ln_u + (n - j) * ln_d
            ST = math.exp(log_ST)
            
            if ST > K:
                final_prob = math.exp(log_final_prob)
                sum_val += final_prob * (ST - K)
                
    return disc * sum_val

def upoutputprice(n: int, Barrier: float, S: float, K: float, r: float, v: float, T: float, log_C: LogCoefficients) -> float:
    """GeliÅŸmiÅŸ Stabilite: Up-and-Out Put opsiyonu fiyatlamasÄ±."""
    dt, u, d, p, q, disc, ln_u, ln_d = _calculate_common_params(n, r, v, T)

    ln_p = math.log(p)
    ln_q = math.log(q)
    ln_S = math.log(S)

    beta = (math.log(K / S) - n * ln_d) / (ln_u - ln_d)

    # Reflection index H (Up-Out iÃ§in H > 0)
    H = 0
    Su = S * u
    for i in range(1, n + 1):
        if Su > Barrier:
            H = i
            break
        Su *= u

    sum_val = 0.0
    end_j = math.ceil(beta)
    end_j = max(0, min(n, int(end_j)))

    # OpenMP yerine basit dÃ¶ngÃ¼
    for j in range(end_j + 1):
        # Main coefficient is C(n, j)
        log_coeff_main = log_C[j]
        
        # Reflected coefficient is C(n, n-j + H)
        idx_sub = n - j + H
        log_coeff_sub = log_C[idx_sub] if (0 <= idx_sub <= n) else NEGATIVE_INF

        # Apply Reflection Principle in LOG space for stability
        log_coeff_ratio = log_coeff_sub - log_coeff_main # ln(B/A)
        
        if log_coeff_ratio < 0.0:
            
            # Stable calculation of ln(A - B)
            log_effective_coeff = log_coeff_main + math.log(1.0 - math.exp(log_coeff_ratio))

            # Calculate log(Probability) (excluding C)
            log_prob_no_C = j * ln_p + (n - j) * ln_q
            
            # Combined log-probability
            log_final_prob = log_effective_coeff + log_prob_no_C
            
            # Calculate Stock Price (ST)
            log_ST = ln_S + j * ln_u + (n - j) * ln_d
            ST = math.exp(log_ST)
            
            if ST < K:
                final_prob = math.exp(log_final_prob)
                sum_val += final_prob * (K - ST)
                
    return disc * sum_val

