# ðŸŒŸ ENHANCED STABILITY: Barrier Options
# ------------------------------------------------------------------

def downoutputprice(n: int, Barrier: float, S: float, K: float, r: float, v: float, T: float, log_C: LogCoefficients) -> float:
    """GeliÅŸmiÅŸ Stabilite: Down-and-Out Put opsiyonu fiyatlamasÄ±."""
    dt, u, d, p, q, disc, ln_u, ln_d = _calculate_common_params(n, r, v, T)

    ln_p = math.log(p)
    ln_q = math.log(q)
    ln_S = math.log(S)

    beta = (math.log(K / S) - n * ln_d) / (ln_u - ln_d)

    # Reflection index H
    H = 0
    Sd = S * d
    for i in range(1, n + 1):
        if Sd < Barrier:
            H = -i
            break
        Sd *= d # Sd = S * d^(i+1)

    sum_val = 0.0
    end_j = math.ceil(beta)
    end_j = max(0, min(n, int(end_j)))

    # OpenMP yerine basit dÃ¶ngÃ¼
    for j in range(end_j + 1):
        log_coeff_main = log_C[j]
        
        # Reflected index for down-and-out options (j - H)
        idx_sub = j - H
        
        # log_C[idx_sub] veya -INFINITY
        log_coeff_sub = log_C[idx_sub] if (0 <= idx_sub <= n) else NEGATIVE_INF

        # Apply Reflection Principle in LOG space for stability
        log_coeff_ratio = log_coeff_sub - log_coeff_main # ln(B/A)
        
        if log_coeff_ratio < 0.0: # log(B) < log(A) implies A > B (valid path: A - B > 0)
            
            # Stable calculation of ln(A - B) = ln(A) + ln(1 - exp(ln(B) - ln(A)))
            # A, B yerine log_coeff_main ve log_coeff_sub'Ä± kullanÄ±yoruz.
            log_effective_coeff = log_coeff_main + math.log(1.0 - math.exp(log_coeff_ratio))

            # Calculate log(Probability) (excluding C)
            log_prob_no_C = j * ln_p + (n - j) * ln_q
            
            # Combined log-probability
            log_final_prob = log_effective_coeff + log_prob_no_C
            
            # Calculate Stock Price (ST)
            log_ST = ln_S + j * ln_u + (n - j) * ln_d
            ST = math.exp(log_ST)
            
            if ST < K:
                final_prob = math.exp(log_final_prob)
                sum_val += final_prob * (K - ST)
                
    return disc * sum_val

def downoutcallprice(n: int, Barrier: float, S: float, K: float, r: float, v: float, T: float, log_C: LogCoefficients) -> float:
    """GeliÅŸmiÅŸ Stabilite: Down-and-Out Call opsiyonu fiyatlamasÄ±."""
