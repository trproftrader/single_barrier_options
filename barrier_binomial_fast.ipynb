    ln_p = math.log(p)
    ln_q = math.log(q)
    ln_S = math.log(S)
    
    alfa = (n / 2.0) - (math.log(S / K) / (2.0 * v * math.sqrt(T / n)))

    # Reflection index H (Down-Out iÃ§in H < 0)
    H = 0
    Sd = S * d
    for i in range(1, n + 1):
        if Sd < Barrier:
            H = -i
            break
        Sd *= d

    sum_val = 0.0
    start_j = math.ceil(alfa)
    start_j = max(0, min(n + 1, int(start_j)))
    
    if start_j > n:
        return 0.0

    # OpenMP yerine basit dÃ¶ngÃ¼
    for j in range(start_j, n + 1):
        log_coeff_main = log_C[j]
        
        # Reflected index for down-and-out options (j - H)
        idx_sub = j - H
        log_coeff_sub = log_C[idx_sub] if (0 <= idx_sub <= n) else NEGATIVE_INF

        # Apply Reflection Principle in LOG space for stability
        log_coeff_ratio = log_coeff_sub - log_coeff_main # ln(B/A)
        
        if log_coeff_ratio < 0.0:
            
            # Stable calculation of ln(A - B)
            log_effective_coeff = log_coeff_main + math.log(1.0 - math.exp(log_coeff_ratio))

            # Calculate log(Probability) (excluding C)
            log_prob_no_C = j * ln_p + (n - j) * ln_q
            
            # Combined log-probability
            log_final_prob = log_effective_coeff + log_prob_no_C
            
            # Calculate Stock Price (ST)
            log_ST = ln_S + j * ln_u + (n - j) * ln_d
